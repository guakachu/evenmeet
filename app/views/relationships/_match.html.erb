<div class="d-flex swipe-match" style="z-index: 9999999;">
  <div class="text-match mb-4">
    <p>It's a</p>
    <span class="menu-highlight-container-2"><span class="highlight">MAAATCH</span></span>
   </div>

  <div class="d-flex match">
    <%= cl_image_tag target.user.photos.first.key, width: 1000, crop: :fill, class: "avatar", style: 'z-index: 2' %>
    <%= cl_image_tag current.user.photos.first.key, width: 1000, crop: :fill, class: "avatar", style: 'z-index: 2' %>
  </div>

  <div class="chat mt-4">
    <p>Don't wait... Go and find <%= target.user.first_name.capitalize %>!</p>
    <%= link_to "Go!", challenge_path(challenge), class: 'join-button', style: 'z-index: 9999999;text-decoration:none' %>
    <span data-action="click->modal#close" class="button-transparent">Keep swiping</span>
  </div>

</div>

<div style="width: 100%; height: 100vh; overflow: hidden;" class="position-absolute">
  <div style="width: 100%; height: 100%;" class="position-relative">
    <lottie-player src="https://assets6.lottiefiles.com/packages/lf20_ZF8J0eEWCR.json" background="transparent"  speed="0.7"
            style="width: 300%;
                  height: 100vh;
                  position: absolute;
                  z-index: 1;
                  left: -100%;" loop autoplay></lottie-player>
  </div>
</div>
